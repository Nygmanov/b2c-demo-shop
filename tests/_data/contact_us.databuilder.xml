<?xml version="1.0"?>
<transfers
    xmlns="spryker:transfer-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="ContactUs">
        <property name="fullName" type="string" dataBuilderRule="realText(20, 2)"/>
        <property name="email" type="string" dataBuilderRule="realText(20, 2)"/>
        <property name="message" type="string" dataBuilderRule="realText(20, 2)"/>
        <property name="isProcessed" type="bool" dataBuilderRule="boolean"/>
    </transfer>

</transfers>
