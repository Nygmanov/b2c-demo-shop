<?xml version="1.0"?>
<database xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="zed"
          xsi:noNamespaceSchemaLocation="http://static.spryker.com/schema-01.xsd"
          namespace="Orm\Zed\ContactUs\Persistence" package="src.Orm.Zed.ContactUs.Persistence">

    <table name="pyz_contact_us" idMethod="native" allowPkInsert="true" phpName="PyzContactUs" identifierQuoting="true">
        <column name="id_contact_us" required="true" type="INTEGER" primaryKey="true" autoIncrement="true"/>
        <column name="full_name" required="true" type="VARCHAR" size="255"/>
        <column name="email" required="true" type="VARCHAR" size="255"/>
        <column name="message" required="true" type="LONGVARCHAR"/>
        <column name="is_processed" required="true" type="BOOLEAN"/>
        <id-method-parameter value="pyz_contact_us_pk_seq"/>
    </table>
</database>
